<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="at.fhtw.view.LogListController"
>

    <VBox maxWidth="Infinity">
        <HBox maxWidth="Infinity" VBox.vgrow="ALWAYS">
            <Button onMouseClicked="#logAddClicked" text="Add" HBox.hgrow="ALWAYS" maxWidth="Infinity"/>
            <Button onMouseClicked="#logDeleteClicked" text="Delete" HBox.hgrow="ALWAYS" maxWidth="Infinity"/>
        </HBox>

        <ListView fx:id="logListView" VBox.vgrow="ALWAYS"/>
    </VBox>

</AnchorPane>
