<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="at.fhtw.view.LogsController" hgap="10" vgap="10" gridLinesVisible="true">

    <fx:include source="logList.fxml"
                fx:id="logList"
                GridPane.columnIndex="0" GridPane.rowIndex="0"
                GridPane.columnSpan="1" GridPane.rowSpan="1"/>

    <fx:include source="logDetail.fxml" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"
                fx:id="logDetail"
                GridPane.columnIndex="1" GridPane.rowIndex="0"
                GridPane.columnSpan="2" GridPane.rowSpan="1"/>
</GridPane>
